          <argument>
            <kind>1</kind>
            <string>if(!dead){
    if (keyboard_check(ord('D'))){
        image_speed = 0.5;
        hspeed = pSpeed;
        direction = 0;
    }else if (keyboard_check(ord('A'))){
        image_speed = 0.5;
        hspeed = pSpeed;
        direction = 180;
    }else{
        if(!reflect){
            image_speed = 0;
            image_index = 0;
        }
        direction = 0;
        hspeed = 0;
    }
    
    if(hit){
        hitCount++;
        sprite_index = playerSpriteHit;
    }else if(reflect){
        sprite_index = playerSpriteReflect;
        image_speed = 1;
        if(startAnimation){
            image_index = 0;
            startAnimation = false;
        }else{
            if(image_index == 5){
                reflect = false;
                startAnimation = true;
            }
        }
    }else{
        switch(player_sprite){
            case 1: sprite_index = sp_antiKnight; break;
            case 2: sprite_index = sp_antiArcher; break;
            case 3: sprite_index = sp_antiMage; break;
        }
    }
}else{
    sprite_index = sp_death;
    direction = 0;
    hspeed = 0;
    if(startAnimation){
        image_index = 0;
        image_speed = 0.5;
        startAnimation = false;
    }
    if(image_index == 5){
        image_speed = 0;
        instance_create(room_width/2-128,room_height/2-80,obj_message);
    }
}


if(hitCount &gt;=10){
    hitCount = 0;
    hit = false;
}


if (power_count &gt;= 5 &amp;&amp; player_sprite == 2){
    with(obj_shootIcon){
        image_index = 0;
    }
    power_up = true;
}

if (slowMP_count &gt;= 5){
    canSlowMotion = true;
    slowMP_count = 0;
    with(obj_slowMIcon){
        image_index = 0;
    }
}

if(!canSlowMotion){
    with(obj_slowMIcon){
        image_index = 1;
    }
}

if(player_sprite == 1){
    pSpeed = 12;
}else{
    pSpeed = 8;
}

var tnts = instance_number(obj_tnt);

if(tnts == 0 &amp;&amp; !win){
    win = true;
    audio_sound_gain(battleSound,0.4,0);
    instance_create(room_width/2-128,room_height/2-80,obj_winMessage);
}


